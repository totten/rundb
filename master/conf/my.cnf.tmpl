!include %%COMMON%%/conf/my.cnf

[client]
#password	= your_password
port		= 3330
socket		= %%MYSQL_BASE%%/mysql.sock

[mysqld]
port		= 3330
socket		= %%MYSQL_BASE%%/mysql.sock
#tmpdir		= /tmp/
server-id	= 1
log-bin		= %%MYSQL_BASE%%/log/mysql-bin
binlog_format	= mixed

